{
  "name": "click/laravel-prometheus-metrics",
  "description": "Framework-agnostic Laravel middleware for Prometheus metrics collection",
  "version": "1.0.0",
  "autoload": {
    "psr-4": {
      "Click\\LaravelPrometheus\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Click\\LaravelPrometheus\\Providers\\PrometheusServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.3",
    "illuminate/container": "^10.0",
    "illuminate/support": "^10.0",
    "illuminate/http": "^10.0",
    "illuminate/routing": "^10.0",
    "illuminate/contracts": "^10.48"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "friendsofphp/php-cs-fixer": "^3.75"
  }
}
